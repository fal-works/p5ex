{
  "name": "p5ex",
  "description": "An extension of p5.js.",
  "version": "0.1.1",
  "author": "FAL",
  "dependencies": {
    "no-more-for-loops": "^0.1.0"
  },
  "devDependencies": {
    "jsdoc": "^3.5.5",
    "minami": "^1.2.3",
    "rollup": "^0.53.4",
    "rollup-plugin-cleanup": "^2.0.0",
    "rollup-plugin-node-resolve": "^3.0.2",
    "rollup-plugin-typescript2": "^0.10.0",
    "tslint": "^5.9.1",
    "tslint-config-airbnb": "^5.3.0",
    "typescript": "^2.6.2",
    "uglify-es": "^3.3.8"
  },
  "files": [
    "lib",
    "types",
    "doc"
  ],
  "jsnext:main": "lib/p5ex.mjs",
  "license": "MIT",
  "main": "lib/p5ex.js",
  "module": "lib/p5ex.mjs",
  "peerDependencies": {
    "p5": ">=0.5.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fal-works/p5ex"
  },
  "scripts": {
    "build": "npm-run-all rollup doc uglify:umd uglify:es",
    "doc": "jsdoc -d ./doc/ -c ./jsdoc.conf.json -R ./README.md -t ./node_modules/minami/ ./lib/p5ex.mjs",
    "rollup": "rollup -c",
    "test": "echo \"Error: no test specified\" && exit 1",
    "uglify:es": "uglifyjs ./lib/p5ex.mjs --compress --mangle --screw-ie8 --output ./lib/p5ex.min.mjs --comments --ecma 6",
    "uglify:umd": "uglifyjs ./lib/p5ex.js --compress --mangle --screw-ie8 --output ./lib/p5ex.min.js --comments"
  },
  "types": "types/p5ex.d.ts"
}
